#include<stdio.h>
int main()
{
	int n;
	printf("Enter the week number:");
	scanf("%d",&n);
	if(n==1)
	printf("Monday");
	else if(n==2)
	printf("Thuesday");
	else if(n==3)
	printf("Wensday");
	else if(n==4)
	printf("Thursday");
	else if(n==5)
	printf("Friday");
	else if(n==6)
	printf("Saturday");
	else if(n==7)
	printf("Sunday");
	else
	printf("There are only 7 days in a week");
}
